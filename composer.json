{
  "name": "nitsan/ns-theme-cleanblog",
  "type": "typo3-cms-extension",
  "description": "T3 Clean Blog Free TYPO3 Template is a premium TYPO3 theme for personal or company blog with minimal & clean design, super easy settings, light, and ultra-fast page speed. Live-Demo: https://demo.t3planet.com/?theme=t3t-cleanblog PRO version: https://t3planet.com/t3-clean-blog-free-typo3-template",
  "keywords": [
    "TYPO3 CMS",
    "blog",
    "bootstrap",
    "responsive",
    "frontend",
    "theme",
    "web"
  ],
  "homepage": "https://github.com/nitsan-technologies/ns_theme_blog/",
  "authors": [
    {
        "name": "Team NITSAN",
        "email": "info@nitsan.in",
        "role": "Developer",
        "homepage": "http://www.nitsan.in/"
    }
  ],
  "support": {
    "issues": "https://github.com/nitsan-technologies/ns_theme_blog/issues"
  },
  "license": "MIT",
  "require": {
    "typo3/cms-core": "^10.0  || ^11.0 ",
    "georgringer/news": "^9.0.0",
    "nitsan/ns-basetheme": ">=10.0.0",
    "nitsan/ns-news-comments": "^5.0.0"
  },
  "replace": {
      "typo3-ter/ns-theme-cleanblog": "self.version"
  },
  "config": {
    "vendor-dir": ".Build/vendor/",
    "bin-dir": ".Build/bin/"
  },
  "extra": {
    "typo3/cms": {
        "cms-package-dir": "{$vendor-dir}/typo3/cms",
        "web-dir": ".Build/public",
        "extension-key": "ns_theme_cleanblog"
    }
  }
}
